
@{
    ViewBag.Title = "ApprovalInbox";
    Layout = "~/Views/FlightSearch/Flights_Layout.cshtml";
}

<div class="container">
    <div class="dsh-sum-cont-ovrapr" style="display:block;">
        <div id="future-bookings-summary" style="display:block;">
            <div class="grd-heading">
                Active Approvals
            </div>
        </div>
    </div>
    <div id="DetContent" style="display:none;">
        <div class="row" style="border:1px solid lightgrey;margin-top:15px;margin-bottom:15px;background:white;">
            <div class="row" style="background:#f9f9f9;padding:15px;">
                <div class="col-md-10">
                    VIEW BOOKING HISTORY
                </div>
                <div class="col-md-2 PrimaryColor1" style="text-align:right;">
                    <i class="fa fa-chevron-circle-left PrimaryColor1" aria-hidden="true" style="font-size:20px;cursor:pointer;"></i>
                </div>
            </div>
            <div data-bind="with:CurrentViewHistory">


                <div style="width: 100%; text-align: right;">
                    <div class="PrimaryColor1" style="padding: 5px 20px 5px 0px; border-image: none; margin-top: 10px; cursor: pointer;">
                        <i class="fa fa-print PrimaryColor1" aria-hidden="true"></i> PRINT
                    </div>
                </div>
                <div style="width: 100%;" id="histPrnt">
                    <br />
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_content">
                                    <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th style="font-weight:normal;">Message</th>
                                                <th style="font-weight:normal;">Timestamp</th>
                                            </tr>
                                        </thead>
                                        <tbody style="font-size:12px;">
                                            <tr>
                                                <td>gfdfg</td>
                                                <td>timestamp</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>



            </div>
        </div>

        <div style="width:100%;font-family:'Roboto', sans-serif!important;background:white;">

        </div>
    </div>
</div>
<a href="@Url.Action("AppDet","ApprovalDetail")"><span type="reset" class="btn kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light srchBtn">select</span></a>
